USE GRAPH LastFM

CREATE SCHEMA_CHANGE JOB lastfm_schema FOR GRAPH LastFM {
	ADD VERTEX User(PRIMARY_ID id INT, role INT DEFAULT 0) WITH PRIMARY_ID_AS_ATTRIBUTE="true";
    ADD VERTEX Item(PRIMARY_ID id INT, role INT DEFAULT 1) WITH PRIMARY_ID_AS_ATTRIBUTE="true";
	ADD UNDIRECTED EDGE Interact(FROM User, TO Item, is_train BOOL, is_test BOOL);
}

RUN SCHEMA_CHANGE JOB lastfm_schema

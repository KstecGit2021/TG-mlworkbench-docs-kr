set exit_on_error = "false"
use graph Ethereum
CREATE LOADING JOB Load_Ethereum FOR GRAPH Ethereum {
      LOAD "/home/tigergraph/tigergraph/data/gui/loading_data/account.csv" TO VERTEX Account VALUES($0, $1, _, _, _, _, _, _, _, _, _, _) USING SEPARATOR=",", HEADER="true", EOL="\n";
      LOAD "/home/tigergraph/tigergraph/data/gui/loading_data/transaction.csv" TO EDGE Transaction VALUES($0, $1, $2, $3) USING SEPARATOR=",", HEADER="true", EOL="\n";
}
RUN LOADING JOB Load_Ethereum
DROP JOB Load_Ethereum
set exit_on_error = "true"
